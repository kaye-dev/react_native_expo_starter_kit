{
  "expo": {
    "owner": "kaye_dev", // Expoのアカウント名
    "name": "StarterKit", // アプリの名前
    "orientation": "portrait", // アプリの縦向き・横向きの設定
    "slug": "starter-kit", // publishした時のURL名
    "scheme": "starter-kit", // 例: demoを設定したとき => demo://URL
    "version": "0.0.1",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
